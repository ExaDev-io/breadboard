{
  "dependencies": {
    "@actions/artifact": "2.1.1",
    "@actions/cache": "3.2.4",
    "@actions/core": "1.10.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.0",
    "@actions/glob": "0.4.0",
    "@actions/http-client": "2.2.0",
    "@actions/io": "1.1.3",
    "@actions/tool-cache": "2.0.1",
    "@vercel/ncc": "^0.38.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "tsx": "^4.7.0",
    "typescript": "5.3.3"
  },
  "scripts": {
    "build": "rm dist/index.cjs ; tsc && ncc build src/index.ts -o dist",
    "test:ts": "cd ../.. ; GITHUB_REPOSITORY=owner/repo tsx -e '(async () => { await require(\"./.github/scripts/src/index.ts\")()})()'",
    "test:js": "cd ../.. ; GITHUB_REPOSITORY=owner/repo node -e 'require(\"./.github/scripts/dist/index.js\")()'",
    "watch": "npm run build -- --watch"
  }
}
