{
  "name": "@google-labs/breadboard-invoker-api-functions",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "npm run build && npx functions-framework --target=api",
    "deploy": "gcloud functions deploy breadboard-invoke --entry-point=api --gen2 --region=us-central1 --runtime=nodejs20 --trigger-http --allow-unauthenticated",
    "gcp-build": "npm run build"
  },
  "engines": {
    "node": "20"
  },
  "main": "build/index.js",
  "type": "module",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-labs/breadboard": "^0.16.0",
    "@google-labs/core-kit": "^0.6.0",
    "@google-labs/template-kit": "^0.2.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "^4.9.0"
  }
}
