{
  "name": "@google-labs/utility-kit",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "version": "0.0.1",
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "prepack": "npm run build",
    "generate:docs": "wireit",
    "test": "wireit",
    "build": "wireit",
    "build:tsc": "wireit",
    "lint": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../breadboard:build",
        "build:tsc"
      ]
    },
    "build:tsc": {
      "command": "tsc -b",
      "env": {
        "FORCE_COLOR": "1"
      },
      "dependencies": [
        "../breadboard:build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "dist/"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "c8 node --test --test-reporter spec --loader ts-node/esm tests/**/*.test.ts",
      "env": {
        "FORCE_COLOR": "1"
      }
    },
    "lint": {
      "command": "eslint . --ext .ts",
      "env": {
        "FORCE_COLOR": "1"
      },
      "files": [
        "src/**/*.ts",
        "tests/**/*.ts",
        "../../.eslintrc.json"
      ],
      "output": []
    },
    "generate:docs": {
      "command": "typedoc --plugin typedoc-plugin-markdown",
      "dependencies": [
        "build:tsc"
      ],
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "../../core/tsconfig/base.json"
      ],
      "output": [
        "docs/api/"
      ]
    }
  },
  "repository": {
    "directory": "packages/utility-kit",
    "type": "git",
    "url": "https://github.com/breadboard-ai/breadboard.git"
  },
  "files": [
    "dist/src"
  ],
  "ava": {
    "timeout": "30s",
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "keywords": [],
  "author": "Google Labs Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/breadboard-ai/breadboard/issues"
  },
  "homepage": "https://github.com/breadboard-ai/breadboard/tree/main/packages/utility-kit#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@google-labs/tsconfig": "^0.0.1",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.20.0",
    "ava": "^5.2.0",
    "c8": "^9.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wireit": "^0.14.4"
  },
  "dependencies": {
    "@google-labs/breadboard": "^0.12.0"
  },
  "engines": {
    "node": ">=19.0.0"
  }
}
